a = float(input('Enter first number:  '))
b = float(input('Enter second number:  '))

addition = a+ b
subtraction = a-b
multiplication = a*b
division = a/b

print(f'Addition: {addition}')
print(f'Subtraction: {subtraction}')
print(f'Multiplication: {multiplication}')
print(f'Division: {division}')



